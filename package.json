{
  "name": "@mnpcmw6444/mongoose-auto-versioning",
  "version": "4.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --minify --format=esm --outfile=dist/esm/index.js && esbuild src/index.ts --bundle --platform=node --minify --format=cjs --outfile=dist/cjs/index.js && tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MNPCMW6444/mongoose-auto-versioning.git"
  },
  "author": "Michael Nusair",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MNPCMW6444/mongoose-auto-versioning/issues"
  },
  "homepage": "https://github.com/MNPCMW6444/mongoose-auto-versioning#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "esbuild": "^0.19.8",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.8",
    "mongoose": "^8.0.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "immutable": "^5.0.0-beta.4"
  },
  "description": ""
}
